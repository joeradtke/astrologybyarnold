Rails.application.routes.draw do


  resources :line_items

  resources :carts

  devise_for :users

  ActiveAdmin.routes(self)
  get 'about'=>'index#about'

  get 'contact'=>'index#contact'

  get 'astro'=>'astro#index'

  get 'natal'=>'natal#new'
  post 'natal'=>'natal#create', :as=>'natal_create'
