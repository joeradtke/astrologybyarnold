<%= simple_form_for(@natal, url: natal_create_path,:html=>{:class=>'form-horizontal'}) do |f| %>
  <%=f.input :name, autofocus: true %>
  <%=f.input :date, as: :datetime, start_year:1925%>
  <%=f.input :id, :label=>"City" do%>
  <%=f.select :id, Birthplace.all.map {|b| [b.city,b.id]}%>
  <%=f.submit%>
  <%end%>
<% end %>
