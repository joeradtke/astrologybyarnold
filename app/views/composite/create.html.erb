<h3>Composite for <%=@birth.name%> born <%=@birth.date%> at <%=@birthplace.city%> and <%=@birth2.name=%> born <%=@birth2.date%> at <%=@birthplace2.city%></h3>
<%=image_tag @image,:class=>"composite"%>
<%=image_tag @image2,:class=>"composite"%>
<table width="75%" cellpadding="0" cellspacing="0" border="0">
  <tr>
    <td><font color='#0000ff'><b> Planets 1 </b></font></td>
    <td><font color='#0000ff'><b> Longitude </b></font></td> 
    <td><font color='#0000ff'><b> Planets 2 </b></font></td>
    <td><font color='#0000ff'><b> Longitude </b></font></td>
    <td><font color='#0000ff'><b> Composite </b></font></td> 
    <td><font color='#0000ff'><b> Comp. Longitude </b></font></td>
    <td><font color='#0000ff'><b> Comp. House </b></font></td>
  </tr>
  <%=render partial: "planet", collection: @planets%>
</table>
<%=render @pageintro%>
<%=render @pagehouse%>
<%=render @planethouses%>
<%=render @pageasp%>
<%=render @aspecttexts%>
